# x86 instruction descriptions
# Legend:
# cflags	Changed flags
# rflags	Rely from flags
# uflags	Undefined flags
# regs		Changed registers
# op		Changed operands
# type		Type of instruction

[aaa]
cflags: af cf
uflags: of sf zf pf
regs: eax

[aad]
regs: eax
cflags: sf zf pf
uflags: of af cf

[aam]
regs: eax
cflags: sf zf pf
uflags: of af cf

[aas]
cflags: af cf
uflags: of sf zf pf
regs: eax

[adc]
rflags: cf
cflags: of sf zf af cf pf
op: 1

[add]
op: 1
cflags: of sf zf af cf pf

[and]
op: 1
uflags: af
cflags: of cf sf zf pf

[arpl]
op: 1
cflags: zf

[bound]

[bsf]
op: 1
cflags: zf
uflags: cf of sf af pf

[bsr]
op: 1
cflags: zf
uflags: cf of sf af pf

[bt]
cflags: cf
uflags: zf of sf af pf

[btc]
op: 1
cflags: cf
uflags: zf of sf af pf

[btr]
op: 1
cflags: cf
uflags: zf of sf af pf

[bts]
op: 1
cflags: cf
uflags: zf of sf af pf

